<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
　　　<!--markerー-->
   <extension
         id="OriginalMarker"
         name="オリジナルマーカー"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="true">
      </persistent>
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
   </extension>
   <!--edhitor-->
   <extension
         id="Marker2.Editor"
         point="org.eclipse.ui.editors">
   </extension>
   <!--command-->
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="Marker2.command.category"
            name="カテゴリー">
      </category>
      <command
            categoryId="Marker2.command.category"
            id="Marker2.commands.createCommand"
            name="クリエイトコマンド">
      </command>
      <command
            categoryId="Marker2.command.category"
            id="Marker2.commands.deleteCommand"
            name="デリート">
      </command>
   </extension>
   <!--handler-->
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="handlers.CreateHandler"
            commandId="Marker2.commands.createCommand">
      </handler>
      <handler
            class="handlers.DeleteHandler"
            commandId="Marker2.commands.deleteCommand">
      </handler>
   </extension>
   <!--menu-->
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="Marker2.toolbars.create"
               label="クリエイト">
            <command
                  commandId="Marker2.commands.createCommand"
                  icon="icon/群馬.png"
                  id="Marker2.toolbars.createToolbar"
                  style="push"
                  tooltip="マーカー生成">
            </command>
         </toolbar>
         <toolbar
               id="Marker2.toobars.delete"
               label="デリート">
            <command
                  commandId="Marker2.commands.deleteCommand"
                  icon="icon/けいおんjava.png"
                  id="Marker2.toolbars.deretetoolbar"
                  label="デリートマーカー"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
